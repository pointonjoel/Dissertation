gen racism=(resunsafe3_3==1 | resavoid3_3==1 | resinsulted3_3==1 | resattacked3_3==1 | resunsafe6_3==1 | resavoid6_3==1 | resinsulted6_3==1 | resattacked6_3==1 | resunsafe8_3==1 | resavoid8_3==1 | resinsulted8_3==1 | resattacked8_3==1)

