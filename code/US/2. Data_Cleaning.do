***************************1. IMPORT, REFINE & ANALYSE**************************

clear all
set more off

cd "C:\Users\point\OneDrive\Documents\Standard Files\University\Stage 3\EC541\Data\understanding society\US\All\UKDA-6614-stata\analysis" // replace with your directory
*************for help with any file simply type help "command name"*************
use "..\Output\Merged_Data.dta", replace
//pidp is unique, but hhid isn't


* Employment
gen employed = (jbstatt==1 | jbstatt==2) & jbstatt!=. //emp or self-emp
replace employed = . if jbstatt<0 | jbstatt==.
replace employed = 1 if jbstat==1 | jbstat==2
replace employed = 0 if (jbstat>=3 & jbstat<.)
drop jbstatt jbstat

* Female
gen female = 0 if sex==1
replace female = 1 if sex==2
drop sex

* Age (age: BHPS, dvage: US)
replace age = dvage if age==.

drop dvage

* Married (mastat: BHPS, marstat: US)
gen married = (marstat==2 | marstat==3) & marstat!=. //married people and same sex civil partnerships
replace married = . if marstat<0 | marstat==.
replace married = 1 if mastat==1
replace married = 0 if mastat==0 | (mastat>=2 & mastat!=.)

drop mastat marstat

* Disabled (hllt for BHPS, hlsf4d for _bi and _bn (waves 9 and 14), disdif12)
gen disabled = 1 if hllt==1 | hlsf4d==1 | disdif12==1
replace disabled = 0 if hllt==2 | hlsf4d==2 | disdif12==0 | disdif12==-8

drop hllt hlsf4d disdif12

* Part time
rename jbft_dv pt

* Ethnicity (racel: US, and race: BHPS)
gen white = 1 if race==1 | racel==1 | racel==2 | racel==3 | racel==4
replace white = 0 if (race>=2 & race<.) | (racel>=5 & racel<.)

gen black = 1 if race==2 | race==3 | race==4 | racel==5 | racel==6 | racel==14 | racel==15 | racel==16 | racel==17 //black or black-mixed
replace black = 0 if race==1 | (race>=5 & race<.) | (racel>=1 & racel<=4) | (racel>=7 & racel<=13) | racel==97

gen chinese = 1 if race==8 | racel==12
replace chinese = 0 if race==9 | (race>=1 & race<=7) | (racel>=1 & racel<=11) | (racel>=13 & racel<.)

gen asian = 1 if race==5 | race==6 | race==7 | racel==7 | racel==9 | racel==10 | racel==11 | racel==13
replace asian = 0 if race==9 | race==8 | (race>=1 & race<=4) | (racel>=1 & racel<=6) | racel==8 | racel==12 | (racel>=14 & racel<.)

gen nonwhite = 1-white

/*
rename white white2
rename black black2
rename chinese chinese2
rename asian asian2

bysort pidp: egen white = min(white2)
bysort pidp: egen black = min(black2)
bysort pidp: egen chinese = min(chinese2)
bysort pidp: egen asian = min(asian2)
*/
bysort pidp: carryforward white, replace
bysort pidp: carryforward black, replace
bysort pidp: carryforward chinese, replace
bysort pidp: carryforward asian, replace

drop race racel racel_bh //white2 black2 chinese2 asian2

* need to merge paygwc (US) and paygw (BHPS). Same for paynwc and paynw
gen gwkpay = paygl*jbhrs if paygwc==0 //hours woked per week?
replace gwkpay = paygl if paygwc==1
replace gwkpay = paygl/2 if paygwc==2
replace gwkpay = paygl/3 if paygwc==3
replace gwkpay = paygl/4 if paygwc==4
replace gwkpay = paygl/4.33 if paygwc==5
replace gwkpay = paygl/8.66 if paygwc==6
replace gwkpay = paygl/6.5 if paygwc==7
replace gwkpay = paygl/5.78 if paygwc==8
replace gwkpay = paygl/5.2 if paygwc==9
replace gwkpay = paygl/13 if paygwc==13
replace gwkpay = paygl/26 if paygwc==26
replace gwkpay = paygl/52 if paygwc==52
replace gwkpay = paygl/3 if paygwc==90
replace gwkpay = . if paygwc==95
replace gwkpay = . if paygwc==96
replace gwkpay = paygl/paygw if (paygw>0 & paygw<.)

gen ghrpay = gwkpay/jbhrs

drop paygl paygwc paygw


